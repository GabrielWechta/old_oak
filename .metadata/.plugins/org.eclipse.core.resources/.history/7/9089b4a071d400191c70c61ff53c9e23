package com.harman;

public class HIBERNATECLASS implements CommandLineRunner{
	@Autowired
	UserRepository repository;
	
    @Override
    public void run(String... args) throws Exception
    {      
        Optional<EmployeeEntity> emp = repository.findById(2L);
 
       System.out.println("Employee id 2 -> {}", emp.get());
    }
    
}
